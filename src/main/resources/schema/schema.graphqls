type Query {
    user(id: ID!): User
}

type User {
    id: ID
    email: String
    firstNname: String
    lastName: String
    userName: String
    password: String
    address: [Address]
}

type Address {
    id: ID
    address1: String
    address2: String
    city: String
    state: String
    postal: String
}
